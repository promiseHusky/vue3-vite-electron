{
  "name": "my-vue-app",
  "private": true,
  "main": "main.js",
  "version": "0.0.0",
  "description": "a test app",
  "author": {
    "email": "dmhqs@qq.com",
    "name": "hsqa"
  },
  "repository": "https://github.com/promiseHusky/vue-electron.git",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "eleMain": "webpack",
    "start": "concurrently \"electron .\" \"vite\"",
    "dist": "electron-builder",
    "predist": "npm run eleMain",
    "preeleMain": "npm run build",
    "prebuilds": "webpack"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "homepage": "./",
  "build": {
    "appId": "dmhsq-dmhsq-dmhsq",
    "productName": "hsq",
    "copyright": "Copyright Â© 2019 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "./build/"
    },
    "extraMetadata": {
      "main": "./build/main.js"
    },
    "extends": null,
    "publish": [
      "github"
    ],
    "win": {
      "target": [
        "msi",
        "nsis"
      ],
      "icon": "./build/logo.ico",
      "artifactName": "${productName}-Web-Setup-${version}.${ext}",
      "publisherName": "hsq"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false,
      "installerIcon": "./build/logo.ico"
    }
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.1.0",
    "electron-builder": "^22.14.13",
    "electron-updater": "^4.6.5",
    "vite": "^2.8.0",
    "wait-on": "^6.0.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "electron-is-dev": "^2.0.0"
  }
}
